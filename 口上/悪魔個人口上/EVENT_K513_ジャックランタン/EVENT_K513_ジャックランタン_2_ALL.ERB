		;ワイシャツ
		CFLAG:초기옷 = 121
		CFLAG:옷 = 121
		TEQUIP:옷 = 121
		;ズボン
		CFLAG:초기하의 = 204
		CFLAG:하의 = 204
		TEQUIP:하의 = 204
