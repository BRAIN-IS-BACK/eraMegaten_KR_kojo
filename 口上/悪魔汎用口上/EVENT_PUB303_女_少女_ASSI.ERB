;-------------------------------------------------
;122,조수를범하게한다
;-------------------------------------------------
	CASE 122
		SELECTCASE ARG
			;気持ちよくて感じている
			CASE 1
				PRINTFORMW 『앙、아응…너무 기분 좋아%UNICODE(0x2661)%』
				RETURN 1
			;こっちに来てと言うセリフ
			CASE 2
				PRINTFORMW 『자아、이 쪽으로 와！』
				PRINTFORMW 『당신의 것、받아들여・줄・게%UNICODE(0x2661)%』
				RETURN 1
			;大丈夫というセリフ
			CASE 3
				IF 0 == 1
					PRINTFORMW 『사양할거 없어！』
				ELSE	
					PRINTFORMW 『괜찮아！』
					PRINTFORMW 『이ー언니가 리드해줄게！』
				ENDIF	
				RETURN 1
			;범해지며 강한 척을 하는 대사
			;CASE 4
			;	PRINTFORMW 『』
			;	RETURN 1
			;능숙해졌네 대사
			;CASE 5
			;	PRINTFORMW 『』
			;	RETURN 1
		ENDSELECT
		
;-------------------------------------------------
;124,더블펠라
;-------------------------------------------------
	CASE 124
			SELECTCASE ARG
				;함께 기분 좋아지자는 대사
				CASE 1
					PRINTFORMW 『자아、같이 기분 좋게 하자！』
					RETURN 1
				;내쪽이 더 기분 좋게 해줄 께 하는 대사
				CASE 2
					PRINTFORMW 『%CSTR:ASSI:일인칭% 쪽이 더 잘하는걸！』
					PRINTFORMW 『그렇지、%CSTR:ASSI:이인칭%%UNICODE(0x2661)%』
					RETURN 1
				;영향에 대한 대사
				CASE 3
					PRINTFORMW 『그렇지 않아！%CSTR:ASSI:일인칭%도 지지 않는걸！』
					RETURN 1
				;평소보다 느끼고 있구나 라는 대사
				CASE 4
					PRINTFORMW 『%CSTR:ASSI:이인칭%의 자지、평소보다 움찔거리네』
					RETURN 1
				;레즈 같은 대사에 대한 대사
				CASE 5
					IF ABL:ASSI:레즈끼 >= 3
						PRINTFORMW 『에헤헤、%조사처리(CALLNAME,"와")% 같이 하는게 기쁠지도』
						RETURN 1
					ENDIF
					PRINTFORMW 『%CSTR:ASSI:일인칭%랑 하는게 그렇게 좋아？』
					RETURN 1
				;둘의 펠라는 기분 좋지？대사
				CASE 6
					PRINTFORMW 『둘이서 하는 펠라는 기분 좋지？%CSTR:ASSI:이인칭%』
					RETURN 1
			ENDSELECT

